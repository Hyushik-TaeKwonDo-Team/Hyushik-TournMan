@model Hyushik_TournMan_Web.Classes.ViewModels.AdminViewModel
@{
    ViewBag.Title = "Admin";
}
<script>

    $(document).ready(function () {
        $('.tog_click').click(function () {
            $(this).siblings('.tog_down').toggle();
        });
    });
</script>

<h2>Admin Panel</h2>
<div>
    <div>
        @Html.Partial("_Tournaments", Model.TournamentViewModel)
    </div>
    <div>
        @Html.Partial("_importCSV", Model.ImportCsvViewModel)
    </div>
    <div>
        @Html.Partial("_techniqueAdmin", Model.TechniquesViewModel)
    </div>
    <div>
        <div class="well well-sm tog_click"><h3>User Roles</h3></div>
        <div class="tog_down">@Html.Partial("_UserRoles", Model.UserRolesViewModel)</div>
    </div>
</div>